generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model usuario {
  id       Int    @id @unique @default(autoincrement())
  nome     String
  email    String @unique
  cargo    String
  url_foto String
}

model notebook {
  id              Int     @id @unique @default(autoincrement())
  modelo          String
  quantidade      Int
  disponibilidade Boolean
  patrimonio      String
}
